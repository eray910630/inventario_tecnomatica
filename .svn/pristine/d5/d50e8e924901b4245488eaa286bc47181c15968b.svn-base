                        <table ng-init='configPages()' class='table'>
                          <thead>
                            <tr>
                              <th>Id</th>
                              <th>Codigo</th>
                              <th>Nombre</th>
                              <th>Edit</th>
                              <th>Delete</th>

                            </tr>
                          </thead>
                          <tbody>
                            <tr ng-repeat='obj in lista | startFromGrid: currentPage * pageSize | limitTo: pageSize' , ng-click='seleccionarUsuario(usuario.id)'>
                              <td>{$ obj.id $}</td>
                              <td>{$ obj.codigo $}</td>
                              <td>{$ obj.nombre $}</td><td><a href="" ng-click="sel(obj)"><span class="glyphicon glyphicon-pencil "></span></a> </td>
                              <td><a href="" ng-click="delete(obj)"> <span class="glyphicon glyphicon-trash "></span> </a></td>

                            </tr>
                          </tbody>
                         </table>



                           <div class='btn-group'>

                             <button type='button' class='btn btn-default' ng-disabled='currentPage == 0' ng-click='currentPage = currentPage - 1'>&laquo;</button>

                               <button type='button' class='btn btn-default' ng-disabled='currentPage == page.no - 1' ng-click='setPage(page.no)' ng-repeat='page in pages'>{$ page.no $}</button>

                               <button type='button' class='btn btn-default' ng-disabled='currentPage >= lista.length/pageSize - 1' , ng-click='currentPage = currentPage + 1'>&raquo;</button>
                           </div>



            </div>
        </div>


</div>

</div>


                        <tr>
                            <td><strong>ID</strong></td>
                            <td><strong>Codigo</strong></td>
                            <td><strong>Nombre</strong></td>
                            <td><strong>Edit</strong></td>
                            <td><strong>Delete</strong></td>

                        </tr>




                                <tr  ng-repeat="obj in lista" >
